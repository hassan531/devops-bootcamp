challenge 1
[
    echo "dates/time sys start, user login, cpu usage" and save it in logs/filename.log
    cat logs/filename.log #shows all
    head -2 logs/filename.log #shows top 2
    tail -1 logs/filename.log #shows bottom 1
    ls -lh dir/

]

echo prints what you write and forwards it to save in dir/filename
cat displays that you have just save in dir/filename
head/tail shows few line beginning or end of data in filename
show list of files save in the dir
